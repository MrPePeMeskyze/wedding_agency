<figure class="col-lg-3 col-md-3 col-sm-4 col-xs-6">
  <a class="gallery-iteam" href="/photos/<%= album.permalink %>/">

    <%= image_tag(album.image.url(:view_thumbnail), itemprop:"thumbnail", :alt => album.name) %>
    <span class="image-effect"></span>
    <figcaption itemprop="caption description"><%= album.name %></figcaption>
  </a>                         
</figure>