<% @title = @object.title.to_s %>
<% @description = @object.meta_description.to_s %>
<% @keywords = @object.meta_keywords.to_s %>

<%= render partial: "layouts/slider" %> 

<section id="service">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 col-md-12">
        <div class="service-area">
          <div class="section-heading">
            <h2>Услуги</h2>
            <div class="line"></div>
          </div>
          <div class="service-content">
            <div class="row">
            	<%= render partial: "services/service", collection: @services, :as => :service %>
           	</div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>

<%= render partial: "pages/about_us" %> 

<%= render partial: "pages/why_us" %> 

<section id="gallery_main">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 col-md-12 col-sm-12">
        <div class="gallery-area">
          <div class="section-heading">
            <h2>Фотографии</h2>
            <div class="line"></div>
          </div>
          <div class="my-simple-gallery">
            <div class="row">
            	<%= render partial: "photos/photo", collection: @photos, :as => :photo %>       
            </div>
          </div>
          <%= render partial: "photos/gallery" %> 
        </div>
      </div>
    </div>
  </div>
</section>

<section id="video_main">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 col-md-12 col-sm-12">
        <div class="gallery-area">
          <div class="section-heading">
            <h2>Видеозаписи</h2>
            <div class="line"></div>
          </div>
          <div class="my-simple-gallery">
            <div class="row">
              <%= render partial: "videos/video", collection: @videos, :as => :video %>       
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>

<%= render partial: "pages/clients_list" %> 