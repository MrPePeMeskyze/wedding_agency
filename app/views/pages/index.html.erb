    <div class="row">
      <div class="col-lg-12 col-md-12">
        <div class="service-area">
          <div class="section-heading">
            <h2>Услуги</h2>
            <div class="line"></div>
          </div>
          <div class="service-content">
            <div class="row">
            	<%= render partial: "services/service" %> 
           	</div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>

<%= render partial: "pages/about_us" %> 

<%= render partial: "pages/why_us" %> 


<section id="gallery_main">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 col-md-12 col-sm-12">
        <div class="gallery-area">
          <div class="section-heading">
            <h2>Фотографии</h2>
            <div class="line"></div>
          </div>
          <div class="my-simple-gallery">
            <div class="row">
            	<%= render partial: "photos/photo" %>        
            </div>
