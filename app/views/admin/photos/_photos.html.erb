<tr <% if !photos.is_published %> class="not_published" <% end %> > 
	<th style="width:100px">
		<%= link_to "Удалить", { action: "destroy", id: photos.id },
            method: :delete, data: { confirm: 'Вы уверены?' },
            :class => 'text-danger'  %>
	</th>
	<th>
		<%= @photos.name %>
	</th>
	<th>
		<% @photos.each do |photo| %>
	       <%= image_tag photo.image.url(:thumb) %>
	   <% end %>
	</th>
	<th style="width:100px">
		<%= link_to 'Редактировать', edit_admin_photo_path(photos.id), :class => 'text-success' %>
	</th>
</tr> 